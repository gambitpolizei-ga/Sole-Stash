<%- include('../partials/header') %>
<body>
  <img id='title-image' src="/images/solestash-logo.png" height='500px' width='500px'>
    <h2>MY COLLECTION</h2>
    <section id="collection">
		</section>
      <table id="list">
        <thead>
          <tr>
            <th>NAME</th>
            <th>DATE ACQUIRED</th>
          </tr>
        </thead>
      <tbody>
          <% sneakers.forEach(function(s) { %>
          <tr>
            <td><%= s.title %></td>
            <td><%= s.dateAcquired %></td>
            <td><a href="/sneakers/<%= s._id %>">DETAILS</a></td>
          </tr>
          <% }) %>
      </tbody>
    </table>
</body>

<%- include('../partials/footer') %>
